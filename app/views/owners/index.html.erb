<h1>Owners</h1>

<ul id="owners">
  <% @owners.each do |owner| %>
    <li>
      <%= link_to "#{owner.first_name} #{owner.last_name}", owner_path(owner) %>
      <ul>
        <% owner.units.each do |unit| %>
          <li><%= unit.name %></li>
        <% end %>
      </ul>
    </li>
  <% end %>
</ul>
